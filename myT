#include "factor.h"

typedef struct my_node{
	long int data;
	struct my_node *next;
}node;

node *front;
node *rear;

void myNode(long int data)
{
	node *node0;
	node0 = malloc(sizeof(node0));
	node0->data = data;
	node0->next = NULL;

	if (front == NULL && rear == NULL)
	{
		front = rear = node0;
		return;
	}

	rear->next = node0;
	rear = node0;

}



int logic(long int num)
{
	long int mod;
	long int i;
	long int first;
	node *temp;



	for (i = 2; i <= 100; i++)
	{

		mod = num % i;

		if (mod == 0)
		{
			myNode(i);
		}
	}

	/*logic*/
        temp = front;

        first = temp->data;

	return (first);

}
